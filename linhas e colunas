/*
Fa√ßa um programa que contenha 3 linhas e 2 colunas 
*/

#include <iostream>

using namespace std;

int main(){   
    int x[3][2] = {{0,1},{2,3},{4,5}};
    
    for(int i=0; i<3; i++){
        for(int j=0; j<2; j++){
            cout << "Elemento em x[" << i+1 << "][" << j+1 << "]" << x[i][j] << endl;
        }
    }
    return 0;
}
